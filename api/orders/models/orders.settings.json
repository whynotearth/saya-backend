      "type": "string"
    },
    "postal_code": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "amount": {
      "type": "decimal"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone_number": {
      "type": "string"
    },
    "tracking": {
      "type": "string"
    },
    "location": {
      "type": "component",
      "repeatable": true,
      "component": "location.order-location"
    },
    "order": {
      "type": "component",
      "repeatable": true,
      "component": "orders.order"
    },
    "payment_method": {
      "type": "component",
      "repeatable": false,
      "component": "payment-method.payment-method"
    }
  }
