{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "Address"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "place_name": {
      "type": "string"
    },
    "user_location": {
      "type": "component",
      "repeatable": true,
      "component": "location.order-location"
    },
    "Location_json": {
      "type": "json"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "addresses"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zipcode": {
      "type": "integer"
    },
    "floor": {
      "type": "string"
    },
    "where_to_park": {
      "type": "string"
    }
  }
}
